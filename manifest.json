{
  "manifest_version": 3,
  "name": "GitHub Colab Link",
  "version": "1.2",
  "description": "Adds a button to open .ipynb files in Colab from GitHub pull requests and file views.",
  "icons": {
    "16": "logo_16.png",
    "48": "logo_48.png",
    "128": "logo_128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
